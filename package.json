{
  "name": "dkilo",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "copy-docs-files": "copyfiles -u 1 ./src/**/*.yml ./dist",
    "copy-geo-files": "copyfiles -u 1 ./src/**/*.geojson ./dist",
    "build:dev": "rm -rf ./dist && tsc && npm run copy-docs-files && npm run copy-geo-files",
    "build": "rm -rf ./dist && tsc && npm run copy-geo-files",
    "start:dev": "tsx watch --env-file development.env ./src/app.ts",
    "start": "node --env-file production.env ./dist/app.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-useragent": "^1.0.5",
    "@types/node": "^20.12.13",
    "copyfiles": "^2.4.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@turf/turf": "^7.0.0",
    "@types/async": "^3.2.24",
    "async": "^3.2.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-useragent": "^1.0.15",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "joi": "^17.13.3",
    "mssql": "^10.0.4",
    "mysql2": "^3.10.2",
    "qrcode": "^1.5.3",
    "redis": "^4.6.15",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  }
}
